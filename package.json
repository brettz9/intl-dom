{
  "name": "intl-dom",
  "version": "0.9.0",
  "description": "Internationalization allowing interspersing of DOM elements among the localized strings",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "license-badge": "license-badger --corrections --uncategorizedLicenseTemplate \"\\${license} (\\${name} (\\${version}))\" --filteredTypes=nonempty --textTemplate \"License types\n(project, deps, and bundled devDeps)\" --packageJson --production badges/licenses-badge.svg",
    "license-badge-dev": "license-badger --corrections --filteredTypes=nonempty --textTemplate \"License types\n(all devDeps)\" --allDevelopment badges/licenses-badge-dev.svg",
    "license-badges": "npm run license-badge && npm run license-badge-dev",
    "prepublishOnly": "npm run license-badges",
    "filesize-badge": "filesize-badger --filesizeFormat=\"{}\" --file=\"dist/index.umd.js\" badges/filesize-badge.svg",
    "eslint": "eslint --ext=js,md,html .",
    "rollup": "rollup -c",
    "find-matching-locale": "node --require esm server.js",
    "intl-mocha": "node --icu-data-dir=node_modules/full-icu node_modules/mocha/bin/mocha",
    "coverage-badge": "coveradge badges/coverage-badge.svg",
    "mocha": "mocha",
    "open-coverage": "npm run nyc && open-cli http://localhost:8010/coverage/ && npm start",
    "nyc": "echo 'Removing esm cache which seems to blank out istanbul coverage when breaking out early' && rm -Rf node_modules/.cache/esm && nyc npm run mocha && npm run coverage-badge",
    "browserify-chai-as-promised": "browserify --entry node_modules/chai-as-promised/lib/chai-as-promised.js --standalone chaiAsPromised --outfile ./test/browser/vendor/chai-as-promised.js",
    "browser-test": "open-cli http://localhost:8010/test/browser/ && npm start",
    "start": "static -p 8010",
    "test-nocov": "npm run eslint && npm run rollup && npm run mocha",
    "test": "npm run eslint && npm run rollup && npm run nyc"
  },
  "browserslist": [
    "cover 100%"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text",
      "json-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/intl-dom.git"
  },
  "keywords": [
    "i18n",
    "l10n"
  ],
  "author": "Brett Zamir",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brettz9/intl-dom/issues"
  },
  "homepage": "https://github.com/brettz9/intl-dom#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "file-fetch": "^1.4.1",
    "json-6": "^1.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@fintechstudios/eslint-plugin-chai-as-promised": "^3.0.2",
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.8.2",
    "coveradge": "^0.6.0",
    "eslint": "^7.17.0",
    "eslint-config-ash-nazg": "^23.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-chai-expect-keywords": "^2.0.1",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.10",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-mocha-cleanup": "^1.9.1",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-unicorn": "^25.0.1",
    "esm": "^3.2.25",
    "filesize-badger": "^0.3.3",
    "full-icu": "^1.3.1",
    "intl-list-format": "^1.0.3",
    "intl-pluralrules": "^1.2.2",
    "intl-relative-time-format": "^1.0.7",
    "jsdom": "^16.4.0",
    "json5": "^2.1.3",
    "license-badger": "^0.18.0",
    "mocha": "^8.2.1",
    "mocha-badge-generator": "^0.9.0",
    "mocha-multi-reporters": "^1.5.1",
    "node-fetch": "^2.6.1",
    "node-static": "^0.7.11",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.3"
  }
}
