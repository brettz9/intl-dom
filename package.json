{
  "name": "intl-dom",
  "version": "0.1.0",
  "description": "Internationalization allowing interspersing of DOM elements among the localized strings",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "eslint": "eslint --ext=js,md,html .",
    "rollup": "rollup -c",
    "intl-mocha": "node --icu-data-dir=node_modules/full-icu node_modules/mocha/bin/_mocha --exclude=test/*/** --delay --require esm --require chai/register-expect --require test/bootstrap/node.js",
    "mocha": "_mocha --exclude=test/*/** --delay --require esm --require chai/register-expect --require test/bootstrap/node.js --exit",
    "coverage": "echo 'Removing esm cache which seems to blank out istanbul coverage' && rm -Rf node_modules/.cache/esm && nyc --reporter=html --reporter=text _mocha --exclude=test/*/** --delay --require esm --require chai/register-expect --require test/bootstrap/node.js --exit",
    "open-coverage": "npm run coverage && open-cli http://localhost:8010/coverage/ && npm start",
    "nyc": "nyc _mocha --exclude=test/*/** --delay --require esm --require chai/register-expect --require test/bootstrap/node.js --exit",
    "browserify-chai-as-promised": "browserify --entry node_modules/chai-as-promised/lib/chai-as-promised.js --standalone chaiAsPromised --outfile ./test/browser/vendor/chai-as-promised.js",
    "browser-test": "open-cli http://localhost:8010/test/browser/ && npm start",
    "start": "static -p 8010",
    "test": "npm run eslint && npm run rollup && npm run mocha",
    "test-cov": "npm run eslint && npm run rollup && npm run nyc"
  },
  "browserslist": [
    "cover 100%"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/intl-dom.git"
  },
  "keywords": [
    "i18n",
    "l10n"
  ],
  "author": "Brett Zamir",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brettz9/intl-dom/issues"
  },
  "homepage": "https://github.com/brettz9/intl-dom#readme",
  "engines": {
    "node": ">= 10.0.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "file-fetch": "https://github.com/brettz9/file-fetch#head"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.8.1",
    "eslint": "^6.7.2",
    "eslint-config-ash-nazg": "^13.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsdoc": "^18.4.3",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^14.0.1",
    "esm": "^3.2.25",
    "full-icu": "^1.3.0",
    "intl-list-format": "^1.0.3",
    "intl-pluralrules": "^1.1.1",
    "intl-relative-time-format": "^1.0.7",
    "jsdom": "^15.2.1",
    "json-6": "^1.0.4",
    "json5": "^2.1.1",
    "mocha": "^6.2.2",
    "node-static": "^0.7.11",
    "nyc": "^14.1.1",
    "open-cli": "^5.0.0",
    "rollup": "^1.27.9",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.2",
    "typescript": "^3.7.3"
  }
}
