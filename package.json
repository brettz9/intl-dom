{
  "name": "intl-dom",
  "version": "0.1.0",
  "description": "Internationalization allowing interspersing of DOM elements among the localized strings",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "eslint": "eslint --ext=js,md,html .",
    "rollup": "rollup -c",
    "mocha": "mocha --require esm --require chai/register-expect --require test/bootstrap/node.js test/*.js",
    "coverage": "nyc --reporter=html mocha --require esm --require chai/register-expect --require test/bootstrap/node.js test/*.js",
    "open-coverage": "npm run coverage && open-cli http://localhost:8010/coverage/ && npm start",
    "nyc": "nyc mocha --require esm --require chai/register-expect --require test/bootstrap/node.js test/*.js",
    "browserify-chai-as-promised": "browserify --entry node_modules/chai-as-promised/lib/chai-as-promised.js --standalone chaiAsPromised --outfile ./test/browser/vendor/chai-as-promised.js",
    "open": "open-cli http://localhost:8010/test/browser/ && npm start",
    "start": "static -p 8010",
    "test": "npm run eslint && npm run rollup && npm run mocha",
    "test-cov": "npm run eslint && npm run rollup && npm run nyc"
  },
  "browserslist": [
    "cover 100%"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/intl-dom.git"
  },
  "keywords": [
    "i18n",
    "l10n"
  ],
  "author": "Brett Zamir",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brettz9/intl-dom/issues"
  },
  "homepage": "https://github.com/brettz9/intl-dom#readme",
  "engines": {
    "node": ">= 0.1.14"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@mysticatea/eslint-plugin": "^12.0.0",
    "babel-plugin-transform-async-to-promises": "^0.8.14",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.8.1",
    "eslint": "^6.5.1",
    "eslint-config-ash-nazg": "^11.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.12.0",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^12.1.0",
    "esm": "^3.2.25",
    "file-fetch": "^1.2.0",
    "jsdom": "^15.2.0",
    "mocha": "^6.2.2",
    "node-static": "^0.7.11",
    "nyc": "^14.1.1",
    "open-cli": "^5.0.0",
    "rollup": "^1.25.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.2",
    "typescript": "^3.6.4"
  }
}
