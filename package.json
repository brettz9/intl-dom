{
  "name": "intl-dom",
  "version": "0.5.0",
  "description": "Internationalization allowing interspersing of DOM elements among the localized strings",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "eslint": "eslint --ext=js,md,html .",
    "rollup": "rollup -c",
    "find-matching-locale": "node --require esm server.js",
    "intl-mocha": "node --icu-data-dir=node_modules/full-icu node_modules/mocha/bin/_mocha --require esm --require chai/register-expect --require test/bootstrap/node.js --reporter-options configFile=mocha-multi-reporters.json --exit",
    "coverage-badge": "coveradge --color brightgreen badges/coverage-badge.svg",
    "mocha": "_mocha --require esm --require chai/register-expect --require test/bootstrap/node.js --reporter-options configFile=mocha-multi-reporters.json --exit",
    "open-coverage": "npm run nyc && open-cli http://localhost:8010/coverage/ && npm start",
    "nyc": "echo 'Removing esm cache which seems to blank out istanbul coverage' && rm -Rf node_modules/.cache/esm && nyc npm run mocha && npm run coverage-badge",
    "browserify-chai-as-promised": "browserify --entry node_modules/chai-as-promised/lib/chai-as-promised.js --standalone chaiAsPromised --outfile ./test/browser/vendor/chai-as-promised.js",
    "browser-test": "open-cli http://localhost:8010/test/browser/ && npm start",
    "start": "static -p 8010",
    "test": "npm run eslint && npm run rollup && npm run mocha",
    "test-cov": "npm run eslint && npm run rollup && npm run nyc"
  },
  "browserslist": [
    "cover 100%"
  ],
  "nyc": {
    "reporter": [
      "html",
      "text",
      "json-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/intl-dom.git"
  },
  "keywords": [
    "i18n",
    "l10n"
  ],
  "author": "Brett Zamir",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brettz9/intl-dom/issues"
  },
  "homepage": "https://github.com/brettz9/intl-dom#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "file-fetch": "^1.4.1",
    "json-6": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@fintechstudios/eslint-plugin-chai-as-promised": "^1.0.0",
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.8.1",
    "coveradge": "^0.3.0",
    "eslint": "^6.8.0",
    "eslint-config-ash-nazg": "^17.2.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-expect-keywords": "^2.0.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^16.1.1",
    "esm": "^3.2.25",
    "full-icu": "^1.3.1",
    "intl-list-format": "^1.0.3",
    "intl-pluralrules": "^1.1.1",
    "intl-relative-time-format": "^1.0.7",
    "jsdom": "^16.2.0",
    "json5": "^2.1.1",
    "mocha": "^7.0.1",
    "mocha-badge-generator": "^0.4.0",
    "mocha-multi-reporters": "git+https://github.com/brettz9/mocha-multi-reporters.git#missing-reporter",
    "node-fetch": "^2.6.0",
    "node-static": "^0.7.11",
    "nyc": "^15.0.0",
    "open-cli": "^5.0.0",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "typescript": "^3.8.2"
  }
}
